  "semanticHighlighting": true,
  "tokenColors": [
    {
      "name": "Default punctuation",
      "scope": [
        "text",
        "meta",
        "punctuation",
        "punctuation.separator",
        "punctuation.terminator",
        "meta.var.expr punctuation"
      ],
      "settings": {
        "foreground": "#c8d5dd"
      }
    },
    {
      "name": "namespaces",
      "scope": [
        "entity.name.namespace",
        "entity.name.scope",
        "entity.name.type.namespace",
        "entity.name.scope-resolution"
      ],
      "settings": {
        "foreground": "#b1dff1",
        "fontStyle": "bold"
      }
    },
    {
      "name": "JSX Children",
      "scope": ["meta.jsx.children", "text.html.derivative"],
      "settings": {
        "foreground": "#eae3ff"
      }
    },
    {
      "name": "TypeScript Primitive",
      "scope": ["support.type.primitive"],
      "settings": {
        "foreground": "#b4eea7"
      }
    },
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#3c6496"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "meta.var.expr",
        "string constant.other.placeholder",
        "support.other.variable",
        "support.variable",
        "string.other.link",
        "meta.block variable.other"
      ],
      "settings": {
        "foreground": "#affcfc"
      }
    },
    {
      "name": "Object Keys",
      "scope": [
        "meta.object-literal.key",
        "meta.object.member",
        "meta.object.property"
      ],
      "settings": {
        "foreground": "#b0c6f3"
      }
    },
    {
      "name": "Colors",
      "scope": ["constant.other.color"],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#FF5370"
      }
    },
    {
      "name": "Keyword, Storage, Operations, Miscs",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier",
        "constant.other.color",
        "punctuation.definition.tag",
        "punctuation.separator.inheritance.php",
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html",
        "punctuation.section.embedded",
        "keyword.other.template",
        "keyword.other.substitution",
        "variable.language.this"
      ],
      "settings": {
        "foreground": "#4292fa",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Return",
      "scope": ["keyword.control"],
      "settings": {
        "foreground": "#4292fa",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Tag",
      "scope": [
        "entity.name.tag",
        "meta.tag.sgml",
        "markup.deleted.git_gutter"
      ],
      "settings": {
        "foreground": "#00d9ff"
      }
    },
    {
      "name": "Function, Special Method",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "variable.function",
        "support.function",
        "keyword.other.special-method"
      ],
      "settings": {
        "foreground": "#fff5bf",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Numbers",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#daffaa",
        "fontStyle": ""
      }
    },
    {
      "name": "Constant, Function Argument, Tag Attribute, Embedded",
      "scope": [
        "constant.language",
        "support.constant",
        "constant.character",
        "constant.escape",
        "variable.parameter",
        "keyword.other.unit",
        "keyword.other"
      ],
      "settings": {
        "foreground": "#fcdfc3",
        "fontStyle": "italic"
      }
    },
    {
      "name": "String, Symbols, Inherited Class, Markup Heading",
      "scope": [
        "string",
        "constant.other.symbol",
        "constant.other.key",
        "entity.other.inherited-class",
        "markup.heading",
        "markup.inserted.git_gutter",
        "meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js",
        "punctuation.definition.string"
      ],
      "settings": {
        "foreground": "#81eec4"
      }
    },
    {
      "name": "Class, Support",
      "scope": [
        "entity.name",
        "support.type",
        "support.class.definition",
        "support.other.namespace.use.php",
        "meta.use.php",
        "support.other.namespace.php",
        "markup.changed.git_gutter",
        "support.type.sys-types",
        "meta.tag support.class"
      ],
      "settings": {
        "foreground": "#6fffb7"
      }
    },
    {
      "name": "Entity Types",
      "scope": ["support.type"],
      "settings": {
        "foreground": "#B2CCD6"
      }
    },
    {
      "name": "CSS Class and Support & JSON Keys",
      "scope": [
        "source.css support.type.property-name",
        "source.sass support.type.property-name",
        "source.scss support.type.property-name",
        "source.less support.type.property-name",
        "source.stylus support.type.property-name",
        "source.postcss support.type.property-name",
        "support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#a6e6ff"
      }
    },
    {
      "name": "Sub-methods",
      "scope": [
        "entity.name.module.js",
        "variable.import.parameter.js",
        "variable.other.class.js"
      ],
      "settings": {
        "foreground": "#FF5370"
      }
    },
    // {
    //   "name": "Language methods",
    //   "scope": ["variable.language"],
    //   "settings": {
    //     "foreground": "#537bff"
    //   }
    // },
    {
      "name": "entity.name.method.js",
      "scope": ["entity.name.method.js"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#82AAFF"
      }
    },
    {
      "name": "meta.method.js",
      "scope": [
        "meta.object-literal.key.js",
        "meta.class-method.js entity.name.function.js",
        "variable.function.constructor"
      ],
      "settings": {
        "foreground": "#82AAFF"
      }
    },
    {
      "name": "CSS Pseudo elements",
      "scope": ["entity.other.attribute-name.pseudo-element"],
      "settings": {
        "foreground": "#ff8686",
        "fontStyle": "italic"
      }
    },
    {
      "name": "CSS Pseudo elements",
      "scope": ["entity.other.attribute-name.pseudo-class"],
      "settings": {
        "foreground": "#f08fd7",
        "fontStyle": "italic"
      }
    },
    {
      "name": "CSS Brackets",
      "scope": [
        "punctuation.section.function.begin.bracket.round",
        "punctuation.section.function.end.bracket.round"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Attributes",
      "scope": [
        "entity.other.attribute-name",
        "entity.other.attribute-name.class"
      ],
      "settings": {
        "foreground": "#fff5bf",
        "fontStyle": "italic"
      }
    },
    {
      "name": "HTML Attributes",
      "scope": [
        "text.html.basic entity.other.attribute-name.html",
        "text.html.basic entity.other.attribute-name"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#ffe0a7"
      }
    },
    {
      "name": "CSS IDs",
      "scope": ["source.sass keyword.control", "source.css keyword.control"],
      "settings": {
        "foreground": "#c9d9fa"
      }
    },
    {
      "name": "Inserted",
      "scope": ["markup.inserted"],
      "settings": {
        "foreground": "#C3E88D"
      }
    },
    {
      "name": "Deleted",
      "scope": ["markup.deleted"],
      "settings": {
        "foreground": "#FF5370"
      }
    },
    {
      "name": "Changed",
      "scope": ["markup.changed"],
      "settings": {
        "foreground": "#C792EA"
      }
    },
    {
      "name": "Regular Expressions",
      "scope": [
        "string.regexp",
        "string.regexp punctuation.definition.string.begin",
        "string.regexp punctuation.definition.string.end"
      ],
      "settings": {
        "foreground": "#ff6565"
      }
    },
    {
      "name": "Escape Characters",
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#89DDFF"
      }
    },
    {
      "name": "URL",
      "scope": ["*url*", "*link*", "*uri*"],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "name": "Decorators",
      "scope": [
        "tag.decorator.js entity.name.tag.js",
        "tag.decorator.js punctuation.definition.tag.js"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#82AAFF"
      }
    },
    {
      "name": "ES7 Bind Operator",
      "scope": [
        "source.js constant.other.object.key.js string.unquoted.label.js"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#FF5370"
      }
    },
