{
  "name": "Lunar Blue",
  "type": "dark",
  "colors": {
    "editor.background": "#161629",
    "editor.foreground": "#eeffff",
    "activityBarBadge.background": "#1b1b53",
    "sideBarTitle.foreground": "#bbbbbb",
    "sideBar.background": "#111120",
    "activityBar.background": "#404075",
    "statusBar.background": "#232341",
    "titleBar.inactiveBackground": "#232341",
    "titleBar.activeBackground": "#232341",
    "tab.inactiveBackground": "#2d2d50",
    "editorGroup.emptyBackground": "#1f1f4b",
    "editorGroupHeader.tabsBackground": "#1f1f4b",
    "sideBarSectionHeader.background": "#1f1f4b",
    "list.inactiveSelectionBackground": "#1f1f4b",
    "list.hoverBackground": "#2c2c4d",
    "list.activeSelectionBackground": "#2a2a4a",

    // üß≠ Quick pick / command palette
    "quickInput.background": "#1a1a3d",
    "quickInput.foreground": "#dcdcdc",
    "pickerGroup.border": "#2a2a4a",
    "pickerGroup.foreground": "#aaaaaa",

    // üìë Menus and context menus
    "menu.background": "#1a1a3a",
    "menu.foreground": "#dcdcdc",
    "menu.selectionBackground": "#2a2a4a",
    "menu.selectionForeground": "#ffffff",
    "menu.separatorBackground": "#2a2a4a",

    // üß© Dropdowns (like font selector, language mode)
    "dropdown.background": "#1a1a3a",
    "dropdown.foreground": "#dcdcdc",
    "dropdown.border": "#2a2a4a",
    "dropdown.listBackground": "#1a1a3a",

    // üìå Tooltips / hover
    "editorHoverWidget.background": "#1a1a3a",
    "editorHoverWidget.border": "#2a2a4a",

    // üîç Peek / inline references
    "peekView.border": "#353565",
    "peekViewEditor.background": "#1a1a3a",
    "peekViewEditor.matchHighlightBackground": "#35356555",
    "peekViewResult.background": "#14142b",
    "peekViewResult.matchHighlightBackground": "#35356555",
    "peekViewTitle.background": "#1b1b3b",

    // üß† Suggestions / IntelliSense
    "editorSuggestWidget.background": "#1a1a3a",
    "editorSuggestWidget.border": "#2a2a4a",
    "editorSuggestWidget.foreground": "#dcdcdc",
    "editorSuggestWidget.selectedBackground": "#2a2a4a",
    "editorSuggestWidget.selectedForeground": "#ffffff",
    "editorSuggestWidget.highlightForeground": "#82AAFF",
    "editorSuggestWidget.focusHighlightForeground": "#ffffff",

    // üîß Parameter hints
    "editorHoverWidget.foreground": "#dcdcdc",

    // üåø Git decorations (in source control view & gutter)
    "gitDecoration.addedResourceForeground": "#6A9955", // softened green
    // "gitDecoration.modifiedResourceForeground": "#6796E6", // softer blue
    "gitDecoration.deletedResourceForeground": "#C74E39", // muted red
    "gitDecoration.untrackedResourceForeground": "#7FB685", // pale green
    "gitDecoration.ignoredResourceForeground": "#555577",
    "gitDecoration.conflictingResourceForeground": "#D18616",
    "gitDecoration.submoduleResourceForeground": "#8C8CD9",

    // üß± Scrollbar
    "scrollbarSlider.background": "#8888aa33",
    "scrollbarSlider.hoverBackground": "#8888aa44",
    "scrollbarSlider.activeBackground": "#8888aa55",

    // üß© Tabs
    "tab.activeBackground": "#232341",
    "tab.activeForeground": "#ffffff",
    "tab.border": "#2a2a4a",
    "tab.activeBorder": "#404075",
    "tab.unfocusedActiveBorder": "#2d2d50",
    "tab.hoverBackground": "#2a2a4a",
    "tab.unfocusedHoverBackground": "#2a2a4a",
    "tab.inactiveForeground": "#aaaaaa",

    // üìä Editor line highlights and guides
    "editor.lineHighlightBackground": "#2a2a4a44",
    "editorLineNumber.foreground": "#737392",
    "editorLineNumber.activeForeground": "#afafee",
    "editorIndentGuide.background1": "#2a2a4a",
    "editorIndentGuide.activeBackground1": "#404075",

    // üìç Editor borders
    "editorGroup.border": "#2a2a4a",
    "panel.border": "#2a2a4a",
    "sideBar.border": "#2a2a4a",
    "activityBar.border": "#2a2a4a",

    // üì¶ Panels and terminals
    "panel.background": "#1a1a3a",
    "terminal.background": "#161629",
    "terminal.foreground": "#dddddd",
    "terminal.ansiBlack": "#1c1c1c",
    "terminal.ansiRed": "#cc6666",
    "terminal.ansiGreen": "#6A9955",
    "terminal.ansiYellow": "#d7ba7d",
    "terminal.ansiBlue": "#6796E6",
    "terminal.ansiMagenta": "#c586c0",
    "terminal.ansiCyan": "#4ec9b0",
    "terminal.ansiWhite": "#eeeeee",
    "terminal.ansiBrightBlack": "#2e2e2e",
    "terminal.ansiBrightRed": "#ff6c6b",
    "terminal.ansiBrightGreen": "#98c379",
    "terminal.ansiBrightYellow": "#ffec8b",
    "terminal.ansiBrightBlue": "#82AAFF",
    "terminal.ansiBrightMagenta": "#d586e0",
    "terminal.ansiBrightCyan": "#88f0e0",
    "terminal.ansiBrightWhite": "#ffffff",

    // üß≠ Activity bar (selected icon indicator)
    "activityBar.foreground": "#ffffff",
    "activityBar.inactiveForeground": "#aaaaaa",
    "activityBar.activeBorder": "#404075",
    "activityBar.dropBorder": "#82AAFF",

    // üì• Status bar highlights
    "statusBar.foreground": "#cccccc",
    "statusBar.noFolderBackground": "#1a1a3a",
    "statusBar.debuggingBackground": "#7A508B",
    "statusBar.debuggingForeground": "#ffffff",

    // üß≠ Breadcrumbs
    "breadcrumb.foreground": "#8888aa",
    "breadcrumb.activeSelectionForeground": "#ffffff",
    "breadcrumbPicker.background": "#1a1a3a"
